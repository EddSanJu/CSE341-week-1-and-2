# Get all the contacts
GET http://localhost:3000/api/contacts
Accept: application/json

###

# Get a contact by ID
GET http://localhost:3000/api/contacts/65a29a072db7261376618422
Accept: application/json

###

# Create a new contact
POST http://localhost:3000/api/create-contact
Content-Type: application/json
Accept: application/json

{
    "firstName": "John",
    "lastName": "Doe",
    "email": "john.doe@example.com",
    "favoriteColor": "Blue",
    "birthday": "1990-01-01"
}

###

# Update an existing contact
PUT http://localhost:3000/api/update-contact/65a29a072db7261376618422
Content-Type: application/json
Accept: application/json

{
    "firstName": "Jane",
    "lastName": "Doe",
    "email": "jane.doe@example.com",
    "favoriteColor": "Green",
    "birthday": "1991-02-02"
}

###

# Delete a contact
DELETE http://localhost:3000/api/delete-contact/65a29a072db7261376618422
Accept: application/json
